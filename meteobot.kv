                size_hint: [.25, .96]
                spacing: 10
                padding: [5,5,10,40]
                Label:
                    text_size: 220, None
                    text: 'Населеннный пункт:'
                    font_size: 18
                    size_hint: [.9, .02]
                    color: [0, 0, 0, 1]
                TextInput:
                    id: search_box
                    focus: True
                    multiline: False
                    font_size: 18
                    size_hint: [.9, .035]
                    on_text_validate: self.parent.parent.parent.meteo_worker()

                Button:
                    text: 'Поиск'
                    font_size: 18
                    background_color: [(146/100/2.55), (146/100/2.55), (146/100/2.55), 1]
                    size_hint: [.9, .04]
                    on_press: self.parent.parent.parent.meteo_worker()
                Label:
